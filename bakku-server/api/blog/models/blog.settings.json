{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "eventTime": {
      "type": "datetime",
      "required": false
    },
    "tags": {
      "type": "enumeration",
      "enum": [
        "firstyear",
        "secondyear",
        "thirdyear",
        "fourthyear",
        "tech",
        "event",
        "other"
      ],
      "required": true,
      "default": "other"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "authorName": {
      "type": "string",
      "required": true,
      "regex": "^\\w{3,20}$"
    }
  }
}
